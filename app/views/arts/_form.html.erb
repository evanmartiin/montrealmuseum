<%= simple_form_for @art do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :author %>
  <%= f.input :year %>
  <%= f.input :photo %>
  <%= f.association :category %>
  <%= f.association :district %>
  </br>
  <%= f.button :submit %>
  <%= link_to 'Destroy', art, method: :delete, data: { confirm: 'Are you sure?' } %>
<% end %>
